      <setting name="OverlayDeck_WindowPositionX" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="OverlayDeck_WindowPositionY" serializeAs="String">
        <value>-1</value>
      </setting>
      <setting name="OverlayDeck_Scale" serializeAs="String">
